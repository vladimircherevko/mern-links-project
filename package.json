{
  "name": "mern-biger-project",
  "version": "1.0.0",
  "description": "Basic MERN stack from Vladilen MERN-video",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "server": "nodemon app.js",
    "client": "npm run start --prefix client",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run server\" \"npm run client\"",
    "client:install": "npm install --prefix client",
    "client:build": "npm run build --prefix client"
  },
  "keywords": [
    "mern",
    "react",
    "mongodb",
    "express"
  ],
  "author": "Vladimir <vladymyrcher@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "config": "^3.2.5",
    "express": "^4.17.1",
    "express-validator": "^6.3.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.8.9",
    "shortid": "^2.2.15"
  },
  "devDependencies": {
    "concurrently": "^5.0.2",
    "cross-env": "^7.0.0",
    "nodemon": "^2.0.2"
  }
}
